Module ed_misc_coms

  implicit none

  !! misc variables that are NOT shared with BRAMS and thus do not go in misc_coms

  integer :: burnin = 0 !! number of years to ignore demography when starting a run

  integer :: outputMonth = 6 !! month to output annual files

  integer :: restart_target_year = 2000 !! year to read when parsing pss/css with multiple years

  integer :: use_target_year = 0    !! flag specifying whether to search for a target year in pss/css

end Module ed_misc_coms
